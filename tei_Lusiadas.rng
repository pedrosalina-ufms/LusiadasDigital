               <optional>
                  <ref name="tei_Lusiadascountry"/>
               </optional>
               <optional>
                  <ref name="tei_Lusiadasregion"/>
               </optional>
               <optional>
                  <ref name="tei_Lusiadassettlement"/>
               </optional>
               <optional>
                  <ref name="tei_Lusiadasdistrict"/>
               </optional>
               <optional>
                  <ref name="tei_LusiadasgeogName"/>
               </optional>
               <optional>
                  <ref name="tei_Lusiadasinstitution"/>
               </optional>
               <optional>
                  <ref name="tei_Lusiadasrepository"/>
               </optional>
               <zeroOrMore>
                  <ref name="tei_Lusiadascollection"/>
               </zeroOrMore>
               <zeroOrMore>
                  <ref name="tei_Lusiadasidno"/>
               </zeroOrMore>
            </group>
            <zeroOrMore>
               <choice>
                  <ref name="tei_LusiadasmsName"/>
                  <ref name="tei_LusiadasobjectName"/>
                  <ref name="tei_LusiadasaltIdentifier"/>
               </choice>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_Lusiadas-msIdentifier-msId_minimal-constraint-rule-51">
