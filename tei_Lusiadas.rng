            </sch:rule>
         </pattern>
         <ref name="tei_Lusiadasatt.global.attributes"/>
         <ref name="tei_Lusiadasatt.cmc.attributes"/>
         <ref name="tei_Lusiadasatt.fragmentable.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_Lusiadaslg">
      <element name="lg">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(line group) contains one or more verse lines functioning as a formal unit, e.g. a stanza, refrain, verse paragraph, etc. [3.13.1. Core Tags for Verse 3.13. Passages of Verse or Drama 7.2.5. Speech Contents]</a:documentation>
         <group>
            <zeroOrMore>
               <choice>
                  <ref name="tei_Lusiadasmodel.divTop"/>
                  <ref name="tei_Lusiadasmodel.global"/>
               </choice>
            </zeroOrMore>
            <choice>
               <ref name="tei_Lusiadasmodel.lLike"/>
               <ref name="tei_Lusiadasmodel.stageLike"/>
               <ref name="tei_Lusiadasmodel.labelLike"/>
               <ref name="tei_Lusiadasmodel.pPart.transcriptional"/>
               <ref name="tei_Lusiadaslg"/>
            </choice>
            <zeroOrMore>
               <choice>
                  <ref name="tei_Lusiadasmodel.lLike"/>
                  <ref name="tei_Lusiadasmodel.stageLike"/>
                  <ref name="tei_Lusiadasmodel.labelLike"/>
                  <ref name="tei_Lusiadasmodel.pPart.transcriptional"/>
                  <ref name="tei_Lusiadasmodel.global"/>
                  <ref name="tei_Lusiadaslg"/>
               </choice>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="tei_Lusiadasmodel.divBottom"/>
               <zeroOrMore>
                  <ref name="tei_Lusiadasmodel.global"/>
               </zeroOrMore>
            </zeroOrMore>
         </group>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_Lusiadas-lg-atleast1oflggapl-constraint-rule-23">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      context="tei:lg">
               <sch:assert test="count(descendant::tei:lg|descendant::tei:l|descendant::tei:gap) &gt; 0">An lg element must contain at least one child l, lg, or gap element.</sch:assert>
            </sch:rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  id="tei_Lusiadas-lg-abstractModel-structure-lg-in-l-constraint-rule-24">
            <sch:rule xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns="http://www.tei-c.org/ns/1.0"
