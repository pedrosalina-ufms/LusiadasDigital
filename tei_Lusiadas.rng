         <ref name="tei_Lusiadasatt.global.attributes"/>
         <ref name="tei_Lusiadasatt.declaring.attributes"/>
         <ref name="tei_Lusiadasatt.notated.attributes"/>
         <ref name="tei_Lusiadasatt.typed.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="tei_Lusiadasannotation">
      <element name="annotation">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">represents an annotation following the <a xmlns="http://www.w3.org/1999/xhtml"
               xmlns:rng="http://relaxng.org/ns/structure/1.0"
               href="#WADM">Web Annotation Data Model</a>. [17.10. The standOff Container]</a:documentation>
         <group>
            <zeroOrMore>
               <ref name="tei_LusiadasrespStmt"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="tei_LusiadasrevisionDesc"/>
