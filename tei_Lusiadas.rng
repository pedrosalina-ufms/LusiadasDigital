               <ref name="tei_Lusiadaslicence"/>
            </zeroOrMore>
            <zeroOrMore>
               <ref name="tei_Lusiadasmodel.annotationPart.body"/>
            </zeroOrMore>
         </group>
         <ref name="tei_Lusiadasatt.global.attribute.n"/>
         <ref name="tei_Lusiadasatt.global.attribute.xmllang"/>
         <ref name="tei_Lusiadasatt.global.attribute.xmlbase"/>
         <ref name="tei_Lusiadasatt.global.attribute.xmlspace"/>
         <ref name="tei_Lusiadasatt.global.change.attribute.change"/>
         <ref name="tei_Lusiadasatt.global.facs.attribute.facs"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.corresp"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.synch"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.sameAs"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.copyOf"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.next"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.prev"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.exclude"/>
         <ref name="tei_Lusiadasatt.global.linking.attribute.select"/>
         <ref name="tei_Lusiadasatt.global.rendition.attribute.rend"/>
         <ref name="tei_Lusiadasatt.global.rendition.attribute.style"/>
         <ref name="tei_Lusiadasatt.global.rendition.attribute.rendition"/>
         <ref name="tei_Lusiadasatt.global.responsibility.attribute.cert"/>
         <ref name="tei_Lusiadasatt.global.responsibility.attribute.resp"/>
         <ref name="tei_Lusiadasatt.global.source.attribute.source"/>
         <ref name="tei_Lusiadasatt.pointing.attribute.targetLang"/>
         <ref name="tei_Lusiadasatt.pointing.attribute.evaluate"/>
         <attribute name="xml:id">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(identifier) provides a unique identifier for the element bearing the attribute.</a:documentation>
            <data type="ID"/>
         </attribute>
         <attribute name="target">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">specifies the destination of the reference by supplying one or more URI References.</a:documentation>
            <list>
               <oneOrMore>
                  <data type="anyURI">
                     <param name="pattern">\S+</param>
                  </data>
               </oneOrMore>
            </list>
